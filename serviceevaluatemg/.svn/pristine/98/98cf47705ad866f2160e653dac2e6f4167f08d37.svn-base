
*{margin: 0;padding: 0;border: 0;}


.lianxi{width: 647px;height: auto;}
.section1{
	height: 80px;
	background-image: url(../images/u16.png);
	background-repeat: no-repeat;
	background-size: 640px ;
	padding-bottom: 10px;
	text-align: center;
}
.section1 span{
	padding-left: 20px;
	font-size: 35px;
	font-weight: bold;
	font-family:"微软雅黑";
}
.content{width: 643px;height: 480px;display: block;border: 2px solid #7c7c7c;float: left;
	border-bottom: #fff;background: #cdcdff;min-height: 360px;}
.left{width: 128px;height: auto;display: block;float: left;margin: 31px 0 0 12px;}
.left1{width: 100%;height: 182px;display: block;}
.left1 > tu{display: block;width: 100%;height: 126px;}
.left1 > a{width: 128px;height: 42px;background: #666;display: inline-block;margin-top: 1px;
	text-align: center;color: #fff;}
.left1 > a input{width: 50px;height: 12px;border: none;background: #666;color: #fff;}
.left > i{width: 90px; height: 90px;margin: 20px 0 0 20px;float: left;}
.left > b{width: 100%;height: 16px;font-size: 16px;font-family:"微软雅黑";line-height: 16px;text-align: center;
	margin-top: 18px;display: block;float: left;margin-bottom: 10px;}
.right{width: 400px;height: auto;float: right;margin-right: 57px;display: block;}
.phone{width: auto;height: 30px;font-size: 25px;font-family:"微软雅黑";margin-top: 20px;display: block;}
.input{width: 380px;height: 50px;display: block;border: 1px solid #7a7a7a;
	margin-top: 15px;font-size: 26px;font-family:"微软雅黑";font-weight: bold;}
.shuzi{width: 300px;height: 100px;;margin-left:100px;
	display: none;}
.ul1{width: 300px;height: 56px;list-style: none;float: left;}

.ul1 > li{width: 55px;background: #fff;height: 55px;float: left;margin: 0px 0px;font-size: 25px;font-family:"微软雅黑";
	border: 1px solid #787878;line-height: 64px;text-align: center;font-weight: bold;}
.tu1{ position: absolute;
  left: 400px;
  top: 400px;width: 60px;height: 40px;float: left; cursor: pointer;}
.tu2{
	position: absolute;
  left: 500px;
  top: 400px;
	width: 60px;height: 40px;float: left;margin-left:18px;cursor: pointer;}
.size1,.size2,.size3{font-size:30px;font-family:"微软雅黑";color: red}  
.back
{
	position:absolute;
	width:60px;
	height:60px;
	top:100px;
	left: 580px;
}
.service{
	width:160px;
	height:40px;
	float:left;
	text-align:left;
	margin-left:50px;
	line-height:40px;
}