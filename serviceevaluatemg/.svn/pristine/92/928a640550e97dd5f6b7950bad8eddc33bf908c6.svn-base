<%@ page language="java" contentType="text/html; charset=GBK"
    pageEncoding="GBK"%>
<%@taglib prefix="s" uri="/struts-tags" %> 

<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=GBK">
<title>录入服务人员</title>
</head>
<body>
	<h2>输入员工信息</h2><br/>
	<s:form action="staff_add.action">
		员工编号:<s:textfield name="staff.staff_no"/><br>
		用户名:<s:textfield name="staff.username"/><br>
		密码:<s:textfield name="staff.password"/><br>
		姓名:<s:textfield name="staff.name"/><br>
		联系电话:<s:textfield name="staff.phone"/><br>
		角色:<s:textfield name="staff.role_id"/><br>
		岗位:<s:textfield name="staff.post_id"/><br>
		头像:<s:textfield name="staff.photo"/><br>
		<s:submit name="输入"/>
		<s:reset name="重置"/>
	</s:form>
</body>
</html>